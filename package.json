{
  "name": "obsidian-geff",
  "version": "1.0.0",
  "description": "Quick navigation between notes using slots and workspaces, inspired by Harpoon",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.{ts,js,json}"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "navigation",
    "harpoon",
    "slots",
    "workspaces"
  ],
  "author": "Gaff Developer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "obsidian": "latest",
    "prettier": "^3.3.0",
    "ts-jest": "^29.2.0",
    "tslib": "2.6.2",
    "typescript": "^5.4.0"
  },
  "dependencies": {}
}
